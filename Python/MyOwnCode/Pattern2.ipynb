   "source": [
    "def pattern1(n):\n",
    "    for row in range(1,n+1):\n",
    "        print(\" \"*(n-row),end=\"\")\n",
    "        print(\"*\"*(2*row-1))\n",
    "    for row1 in range(n-1,0,-1):\n",
    "        print(\" \"*(n-row1),end=\"\")\n",
    "        print(\"*\"*(2*row1-1))\n",
    "\n",
    "pattern1(5)\n",
    "        \n",
    "            "
   ]
